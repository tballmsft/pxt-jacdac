{
    "name": "jacdac-bluetooth-transport",
    "version": "1.9.24",
    "description": "Support for sending Jacdac packets over BLE.",
    "dependencies": {
        "core": "*",
        "jacdac": "github:tballmsft/pxt-jacdac"
    },
    "yotta": {
        "config": {
            "DEVICE_BLE": 1,
            "JACDAC_BLE_TRANSPORT": 1,
            "SOFTDEVICE_PRESENT": 1,
            "MICROBIT_BLE_ENABLED": 1,
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1
                }
            }
        }
    },
    "files": [],
    "testFiles": [
        "test.ts"
    ],
    "supportedTargets": [
        "microbit",
        "calliopemini"
    ]
}
